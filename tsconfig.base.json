{
  "compilerOptions": {
    /* Code generation */
    "sourceMap": true,
    "downlevelIteration": true,
    "importHelpers": true,
    /* Module resolution */
    "esModuleInterop": true,
    /* Other options */
    "strict": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
